<% page.posts.each(function(post){ %>
<div class="col-12">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">
        <a href="<%- url_for(post.path) %>">
          <b><%= post.title %></b>
        </a>
      </h3>
    </div>
    <div class="panel-body text-muted">
      <%= date(post.date, config.date_format) %>
    </div>
  </div>
</div>
<% }) %>